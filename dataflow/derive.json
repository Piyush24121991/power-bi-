{
	"name": "derive",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText18",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Item_Identifier as string,",
				"          Item_Weight as string,",
				"          Item_Fat_Content as string,",
				"          Item_Visibility as string,",
				"          Item_Type as string,",
				"          Item_MRP as string,",
				"          Outlet_Identifier as string,",
				"          Outlet_Establishment_Year as string,",
				"          Outlet_Size as string,",
				"          Outlet_Location_Type as string,",
				"          Outlet_Type as string,",
				"          Item_Outlet_Sales as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(Item_Weight = iif(isNull(Item_Weight),avg(Item_Weight),Item_Weight)) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(Item_Weight),",
				"     Item_Weight = ERROR_FUNCTION('')) ~> aggregate1"
			]
		}
	}
}