{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText20",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "DelimitedText21",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Item_Identifier as string,",
				"          Item_Weight as float,",
				"          Item_Fat_Content as string,",
				"          Item_Visibility as string,",
				"          Item_Type as string,",
				"          Item_MRP as float,",
				"          Outlet_Identifier as string,",
				"          Outlet_Establishment_Year as string,",
				"          Outlet_Size as string,",
				"          Outlet_Location_Type as string,",
				"          Outlet_Type as string,",
				"          Item_Outlet_Sales as float",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          Item_Identifier as string,",
				"          Item_Weight as string,",
				"          Item_Fat_Content as string,",
				"          Item_Visibility as string,",
				"          Item_Type as string,",
				"          Item_MRP as string,",
				"          Outlet_Identifier as string,",
				"          Outlet_Establishment_Year as string,",
				"          Outlet_Size as string,",
				"          Outlet_Location_Type as string,",
				"          Outlet_Type as string,",
				"          Item_Outlet_Sales as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1 aggregate(Item_Weight = avg(Item_Weight)) ~> aggregate1",
				"aggregate1, source1 join(ERROR_FUNCTION(''),",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}